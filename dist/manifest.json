{
  "manifest_version": 3,
  "name": "LeetCode Company Lists Exporter",
  "version": "1.0",
  "description": "Export LeetCode company question URLs to CSV",
  "permissions": [
    "cookies",
    "downloads"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "LeetCode Company Lists Exporter"
  }
}
